   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "#Answer no :- 3\n",
    "Value of spam[int(int('3' * 2) / 11)] is :- \"d\""
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "#Answer no :- 4\n",
    "Value of spam[-1] is :- \"d\""
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "#Answer no :- 5\n",
    "Value of spam[:2] is :- ['a', 'b']"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "#Answer no :- 6\n",
    "Value of bacon.index('cat') is :- 1\n"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "#Answer no :- 7\n",
    "bacon.append(99)\n",
    "bacon=[3.14,'cat',11,'cat',True,99]"
   ]
